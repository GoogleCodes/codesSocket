<!--pages/login/login.wxml-->
<view class="loginView">
  <form name='pushMsgFm' report-submit bindsubmit='loginForm'>
    <view class='bodyView'>
      <input type='tel' class='appidText' value='{{ uname }}' name='uname' />
    </view>
    <view class='bodyView'>
      <input type='password' class='appidText' value='{{ pword }}' name='pword' />
    </view>
    <button type="primary" class="confirm" form-type="submit">登录</button>
  </form>
</view>
