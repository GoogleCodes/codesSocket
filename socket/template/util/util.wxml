<!--template/util/util.wxml-->
<template name="pageBottom">
  <view class="page-bottom">
    <view class="page-content">
      <view class="wc" bindtap='_shareGizwits' hover-class='wx-hover' hover-start-time='50' hover-stay-time='1000'>
        <view class="wc_list">
          <view class="iconfont icon-share fl"></view>
          <view class="fl">接受分享</view>
        </view>
      </view>
      <navigator url="../person/person" open-type="navigate">
        <view class="wc" hover-class='wx-hover' hover-start-time='50' hover-stay-time='1000'>
        <view class="wc_list">
          <view class="iconfont icon-user fl"></view>
          <view class="fl">个人信息</view>
        </view>
      </view>
      </navigator>
      <navigator url="../forget/forget" open-type="navigate">
        <view class="wc" hover-class='wx-hover' hover-start-time='50' hover-stay-time='1000'>
          <view class="wc_list">
            <view class="iconfont icon-weibiaoti2zhuanhuan fl"></view>
            <view class="fl">修改密码</view>
          </view>
        </view>
      </navigator>
      <view class="wc" bindtap='backLogin' hover-class='wx-hover' hover-start-time='50' hover-stay-time='1000'>
        <view class="wc_list">
          <view class="iconfont icon-user fl"></view>
          <view class="fl">退出登录</view>
        </view>
      </view>
    </view>
  </view>
</template>