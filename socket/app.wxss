/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  font-family: 'Segoe UI', "Helvetica Neue", Helvetica, Arial, sans-serif;
}

@font-face {
	font-family: 'iconfont';
	src: url('iconfont.eot');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.fr {
  float: right;
}

.fl {
  float: left;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-code:before { content: "\e6e8"; }

.icon-liebiao:before { content: "\e6bf"; }

.icon-user:before { content: "\e601"; }

.icon-mima:before { content: "\e641"; }

.icon-deng:before { content: "\e65d"; }

page, .page {
  height: 100%;
  font-family: 'PingFang SC',
                 'Helvetica Neue',
                 Helvetica,
                 'Droid Sans Fallback',
                 'Microsoft Yahei',
                 sans-serif;
}

.page-bottom {
  height: 100%;
  width: 750rpx;
  position: fixed;
  background: #f2f2f2;
  z-index: 0;
}


.page-content {
  padding-top: 300rpx;
  width: 75%;
}

.page-top {
  height: 100%;
  position: fixed;
  width: 750rpx;
  background: rgb(255, 255, 255);
  z-index: 0;
  transition: All 0.4s ease;
  box-shadow:0px 1px 100px #ccc;
  -webkit-transition: All 0.4s ease;
}

.wc {
  padding: 0rpx 0 0rpx 40rpx;
  color: #333;
  height: 40px;
}

.wc_list {
  line-height: 40px;
}

.wx-hover {
  background: #ccc;
  color: #333;
}

.page-top .icon-liebiao {
  font-size: 60rpx;
  height: 40px;
  text-indent: 0.3em;
  line-height: 40px;
  background: #6ba74f;
  color: #fff;
}

.page-top image {
  position: absolute;
  width: 68rpx;
  height: 38rpx;
  left: 20rpx;
  top: 20rpx;
}

.c-state1 {
  transform: rotate(0deg) scale(1) translate(75%, 0%);
  -webkit-transform: rotate(0deg) scale(1) translate(75%, 0%);
}

.c-state2 {
  transform: rotate(0deg) scale(0.8) translate(75%, 0%);
  -webkit-transform: rotate(0deg) scale(0.8) translate(75%, 0%);
}
