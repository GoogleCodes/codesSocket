/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  font-family: 'Segoe UI', "Helvetica Neue", Helvetica, Arial, sans-serif;
}

/* iconfont start */

@font-face {
	font-family: 'iconfont';
	src: url('iconfont.eot');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.fr {
  float: right;
}

.fl {
  float: left;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-weibiaoti2zhuanhuan:before { content: "\e614"; }

.icon-share:before { content: "\e62e"; }

.icon-code:before { content: "\e6e8"; }

.icon-liebiao:before { content: "\e6bf"; }

.icon-return:before { content: "\e676"; }

.icon-user:before { content: "\e601"; }

.icon-mima:before { content: "\e641"; }

.icon-deng:before { content: "\e65d"; }

.icon-shanchu:before { content: "\e634"; }

/* iconfont end */

page, .page {
  height: 100%;
  font-family: 'PingFang SC',
                 'Helvetica Neue',
                 Helvetica,
                 'Droid Sans Fallback',
                 'Microsoft Yahei',
                 sans-serif;
}

.page-bottom {
  height: 100%;
  width: 750rpx;
  position: fixed;
  background: #f2f2f2;
  z-index: 0;
}

.page-content {
  padding-top: 300rpx;
  width: 75%;
}

.page-top {
  height: 100%;
  position: fixed;
  width: 750rpx;
  background: rgb(255, 255, 255);
  z-index: 0;
  transition: All 0.4s ease;
  box-shadow: 0px 1px 100px #ccc;
  -webkit-transition: All 0.4s ease;
}

.wc {
  padding: 0rpx 0 0rpx 40rpx;
  color: #333;
  height: 40px;
  border-bottom: 1px solid #eee;
}

.wc_list {
  height: 40px;
  line-height: 40px;
}

.wc_list .icon-weibiaoti2zhuanhuan, .icon-share {
  margin-right: 10px;
}

.wc_list .icon-share {
  margin-right: 6px;
}

.wc_list .icon-user, .icon-weibiaoti2zhuanhuan{
  font-size: 18px;
}

.icon-share {
  font-size: 22px;
}

.wx-hover {
  background: #ccc;
  color: #333;
}

.page-top .icon-liebiao {
  font-size: 60rpx;
  height: 45px;
  text-indent: 0.3em;
  line-height: 45px;
  background: #6ba74f;
  color: #fff;
}

.page-top image {
  position: absolute;
  width: 68rpx;
  height: 38rpx;
  left: 20rpx;
  top: 20rpx;
}

.c-state1 {
  transform: rotate(0deg) scale(1) translate(75%, 0%);
  -webkit-transform: rotate(0deg) scale(1) translate(75%, 0%);
}

.c-state2 {
  transform: rotate(0deg) scale(0.8) translate(75%, 0%);
  -webkit-transform: rotate(0deg) scale(0.8) translate(75%, 0%);
}

/* loginView start */

.loginView {
  width: 100%;
  background: #fff;
}

.bodyView {
  width: 90%;
  margin: 0% auto;
  border-bottom: 1px solid #f2f2f2;
  height: 40px;
}

.bodyView .iconfont {
  font-size: 46rpx;
  color: #6ba74f;
  line-height: 40px;
  text-align: center;
  height: 40px;
  width: 10%;
}

.loginView {
  position: relative;
}

.loginView .appidText {
  width: 65%;
  height: 40px;
  border-radius: 5px;
  line-height: 40px;
  font-size: 14px;
}

.loginView .bodyView .codeBtn {
  width: 32%;
  line-height: 30px;
  border-radius: 5px;
  margin-top: 5px;
  background: #6ba74f;
  color: #fff;
  font-size: 12px;
  text-align: center;
}

.loginView .labels {
  margin-top: 10px;
}

.loginView .labels .viewLabel {
  height: 35px;
  line-height: 35px;
  color: #6ba74f;
  font-size: 35rpx;
}

.loginView button[type="primary"] {
  width: 95%;
  margin: 15px auto;
  height: 40px;
  position: absolute;
  left: 0px;
  right: 0px;
  line-height: 40px;
  font-size: 14px;
  background: #6ba74f;
  color: #fff;
}

/* loginView end */

/* loading start */

.loading {
  font-size: 1.2rpx;
  font-family: 'microsoft yahei';
}

/* loading end */


/* start */
.msg-list {
}

.msg-item {
  width: 100%;
  height: 150rpx;
  border-bottom: 1rpx solid rgb(233, 233, 233);
  position: relative;
  left:0;
  top:0;
  overflow: hidden;
}

.msg {
  position: absolute;
  width: 100%;
  height: 150rpx;
  left:0;
  top:0;
  z-index: 100;
  background-color: #FFF;
}

.header-img {
  position: absolute;
  width: 110rpx;
  height: 110rpx;
  left: 20rpx;
  top: 20rpx;
  border-radius: 10%;
}

.user-name {
  position: absolute;
  left: 150rpx;
  top: 20rpx;
}

.msg-text {
  position: absolute;
  left: 150rpx;
  bottom: 20rpx;
  font-size: 80%;
  color: rgb(127, 127, 127);
}

.msg-menu {
  position: absolute;
  width: 100%;
  height: 150rpx;
  left:0;
  top:0;
  z-index: 0;
}

.menu-delete {
  position: absolute;
  width: 150rpx;
  height: 148rpx;
  top:1rpx;
  right: 0;
  background-color: rgb(255, 58, 50);
  color:#FFF;
  text-align: center;
  line-height:150rpx;
}

.menu-mark {
  position: absolute;
  width: 200rpx;
  height: 148rpx;
  top:1rpx;
  right: 150rpx;
  background-color: rgb(200, 199, 205);
  color:#FFF;
  text-align: center;
  line-height:150rpx;
}
/* end */