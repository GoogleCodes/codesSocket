page, .page {
  height: 100%;
  font-family: 'PingFang SC',
                 'Helvetica Neue',
                 Helvetica,
                 'Droid Sans Fallback',
                 'Microsoft Yahei',
                 sans-serif;
}

.page-bottom {
  height: 100%;
  width: 750rpx;
  position: fixed;
  background: #f2f2f2;
  z-index: 0;
}

.page-content {
  padding-top: 300rpx;
  width: 75%;
}

.page-top {
  height: 100%;
  position: fixed;
  width: 750rpx;
  background: rgb(235, 235, 235);
  z-index: 0;
  transition: All 0.4s ease;
  box-shadow: 0px 1px 100px #ccc;
  -webkit-transition: All 0.4s ease;
}

.wc {
  padding: 0rpx 0 0rpx 40rpx;
  color: #333;
  height: 40px;
  border-bottom: 1px solid #eee;
}

.wc_list {
  height: 40px;
  line-height: 40px;
}

.wc_list .icon-weibiaoti2zhuanhuan, .icon-share {
  margin-right: 10px;
}

.wc_list .icon-deng {
  margin-right: 10px;
}

.wc_list .icon-share{
  margin-right: 6px;
}

.wc_list .icon-user, .icon-weibiaoti2zhuanhuan {
  font-size: 18px;
}

.icon-share {
  font-size: 22px;
}

.wx-hover {
  background: #ccc;
  color: #333;
}

.page-top .icon-liebiao {
  font-size: 60rpx;
  line-height: 25px;
  color: #333;
  width: 25px;
  padding: 10px;
  position: absolute;
  top: 0px;
  left: 0px;
}

.page-top image {
  position: absolute;
  width: 68rpx;
  height: 38rpx;
  left: 20rpx;
  top: 20rpx;
}

.c-state1 {
  transform: rotate(0deg) scale(1) translate(75%, 0%);
  -webkit-transform: rotate(0deg) scale(1) translate(75%, 0%);
}

.c-state2 {
  transform: rotate(0deg) scale(0.8) translate(75%, 0%);
  -webkit-transform: rotate(0deg) scale(0.8) translate(75%, 0%);
}

/* loginView start */

.loginView {
  width: 100%;
  background: #fff;
}

.bodyView {
  width: 90%;
  margin: 0% auto;
  border-bottom: 1px solid #f2f2f2;
  height: 40px;
}

.bodyView .iconfont {
  font-size: 46rpx;
  color: #6ba74f;
  line-height: 40px;
  text-align: center;
  height: 40px;
  width: 10%;
}

.loginView {
  position: relative;
}

.loginView .appidText {
  width: 65%;
  height: 40px;
  border-radius: 5px;
  line-height: 40px;
  font-size: 14px;
  color: #b3b3b3;
}

.loginView .codeImage {
  width: 90px;
  height: 35px;
  margin: 3px 0px;
}

.loginView .bodyView .codeBtn {
  width: 32%;
  line-height: 30px;
  border-radius: 5px;
  margin-top: 5px;
  background: #6ba74f;
  color: #fff;
  font-size: 12px;
  text-align: center;
}

.loginView .labels {
  margin-top: 10px;
}

.loginView .labels .viewLabel {
  height: 35px;
  line-height: 35px;
  color: #6ba74f;
  font-size: 35rpx;
}

.loginView button[type="primary"] {
  width: 80%;
  margin: 55px auto;
  height: 40px;
  position: absolute;
  left: 0px;
  right: 0px;
  line-height: 40px;
  font-size: 14px;
  background: #7cb242;
  color: #fff;
}

/* loginView end */

/* loading start */

.loading {
  font-size: 1.2rpx;
  font-family: 'microsoft yahei';
}

/* loading end */

/* start */

.msg-list {
}

.msg-item {
  width: 100%;
  height: 150rpx;
  border-bottom: 1rpx solid rgb(233, 233, 233);
  position: relative;
  left: 0;
  top: 0;
  overflow: hidden;
}

.msg {
  position: absolute;
  width: 100%;
  height: 150rpx;
  left: 0;
  top: 0;
  z-index: 100;
  background-color: #fff;
}

.header-img {
  position: absolute;
  width: 110rpx;
  height: 110rpx;
  left: 20rpx;
  top: 20rpx;
  border-radius: 10%;
}

.user-name {
  position: absolute;
  left: 150rpx;
  top: 20rpx;
}

.msg-text {
  position: absolute;
  left: 150rpx;
  bottom: 20rpx;
  font-size: 80%;
  color: rgb(127, 127, 127);
}

.msg-menu {
  position: absolute;
  width: 100%;
  height: 150rpx;
  left: 0;
  top: 0;
  z-index: 0;
}

.menu-delete {
  position: absolute;
  width: 150rpx;
  height: 148rpx;
  top: 1rpx;
  right: 0;
  background-color: rgb(255, 58, 50);
  color: #fff;
  text-align: center;
  line-height: 150rpx;
}

.menu-mark {
  position: absolute;
  width: 200rpx;
  height: 148rpx;
  top: 1rpx;
  right: 150rpx;
  background-color: rgb(200, 199, 205);
  color: #fff;
  text-align: center;
  line-height: 150rpx;
}

/* end */


