/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  font-family: 'Segoe UI',"Helvetica Neue",Helvetica,Arial,sans-serif;
} 

@font-face {
	font-family: 'iconfont';
	src: url('iconfont.eot');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-liebiao:before { content: "\e6bf"; }

.icon-user:before { content: "\e601"; }

.icon-mima:before { content: "\e641"; }

.icon-deng:before { content: "\e65d"; }


page,.page {
  height: 100%;
  font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
}
.page-bottom{
  height: 100%;
  width: 750rpx;
  position: fixed; 
  background: rgb(0, 68, 97);
  z-index: 0;
}
.wc{
  color: white;
  padding: 20rpx 0 20rpx 40rpx;
  border-bottom: 1px solid #ccc;
}
.page-content{
  padding-top: 300rpx;
}
.page-top {
  height: 100%;
  position: fixed;
  width: 750rpx;
  background: rgb(255, 255, 255);
  z-index: 0;
  transition: All 0.4s ease; 
  -webkit-transition: All 0.4s ease;
}

.wx-hover {
  background: #fff;
  color: #333;
}

.page-top .icon-liebiao {
  font-size: 60rpx;
  height: 35px;
  text-indent: 0.3em;
  line-height: 35px;
  background: rgb(251, 249, 254);
}

.page-top image{
  position: absolute;
  width: 68rpx;
  height: 38rpx;
  left: 20rpx;
  top: 20rpx;
}
.c-state1{
  transform: rotate(0deg) scale(1) translate(75%,0%); 
  -webkit-transform: rotate(0deg) scale(1) translate(75%,0%); 
}
.c-state2{
  transform: rotate(0deg) scale(.8) translate(75%,0%); 
  -webkit-transform: rotate(0deg) scale(.8) translate(75%,0%); 
}
